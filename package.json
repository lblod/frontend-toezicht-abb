{
  "name": "frontend-toezicht-abb",
  "version": "0.26.0",
  "private": true,
  "description": "Frontend of the Toezicht ABB application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-toezicht-abb.git"
  },
  "license": "MIT",
  "author": "info@redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "release": "release-it",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "overrides": {
    "ember-cli-babel": "$ember-cli-babel",
    "torii": "1.0.0-beta.1"
  },
  "overridesNotes": {
    "ember-cli-babel": "ember-cli-deploy-ssh-index brings in ember-cli-babel v6 which triggers the build warning",
    "torii": "torii is used by ember-acmidm-login but v1.0.0-beta fixes some Ember 4 issues"
  },
  "dependenciesNotes": {
    "ember-data": "ember-data is pinned on v4.7 until this issue is resolved: https://github.com/emberjs/data/issues/8312"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^2.4.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.5",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@lblod/ember-acmidm-login": "^1.0.0",
    "@lblod/ember-mock-login": "^0.7.0",
    "@lblod/ember-submission-form-fields": "^2.6.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.6.0",
    "ember-cli": "~4.10.0",
    "ember-cli-app-version": "^6.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-display-revisions": "^2.1.2",
    "ember-cli-deploy-revision-data": "^2.0.0",
    "ember-cli-deploy-rsync": "0.0.5",
    "ember-cli-deploy-ssh-index": "^1.0.0",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.3.7",
    "ember-data": "~4.7.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-moment": "^10.0.0",
    "ember-mu-transform-helpers": "^2.0.0",
    "ember-page-title": "^7.0.0",
    "ember-power-select": "^6.0.1",
    "ember-promise-helpers": "^2.0.0",
    "ember-qunit": "^6.1.1",
    "ember-resolver": "^10.0.0",
    "ember-simple-auth": "^4.2.2",
    "ember-source": "~4.10.0",
    "ember-template-lint": "^5.3.1",
    "ember-truth-helpers": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "lodash.camelcase": "4.3.0",
    "moment": "^2.29.4",
    "prettier": "^2.8.3",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "rdflib": "^2.2.26",
    "release-it": "^15.5.0",
    "sass": "^1.23.7",
    "tracked-built-ins": "^3.1.0",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
